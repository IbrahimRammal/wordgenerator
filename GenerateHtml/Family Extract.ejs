<form method="POST" action="/api/posts/data?lang=<%= obj.type %>&doc=<%= obj.caption %>&id=<%= obj.client.id %>">
    <div class="form-group">
        <h8 class="m-0 font-weight-bold text-secondary"><%= obj.s1.caption %></h8>
        <div class="form-row">
            <div class="form-group col">
                <input type="text" class="form-control" id="s1f1" name="s1_f1" placeholder="<%= obj.s1.f1.caption %>"
                    value="<%= obj.s1.f1.value %>">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" id="s1f2" name="s1_f2" placeholder="<%= obj.s1.f2.caption %>"
                    value="<%= obj.s1.f2.value %>">
            </div>
        </div>
    </div>
    <hr class="sidebar-divider">
    <div class="form-group">
        <h8 class="m-0 font-weight-bold text-secondary"><%= obj.s1.f3.caption %></h8>
        <div class="form-row">
            <div class="form-group col">
                <input type="text" class="form-control" name="s1_f3_s1" id="s1f3s1" placeholder="<%= obj.s1.f3.s1.caption %>"
                    value="<%= obj.s1.f3.s1.value %>">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s1_f3_s2" id="s1f3s2" placeholder="<%= obj.s1.f3.s2.caption %>"
                    value="<%= obj.s1.f3.s2.value %>">
            </div>
        </div>
    </div>
    <hr class="sidebar-divider">
    <div class="form-group">
        <div class="form-row">
            <div class="form-group col">
                <input type="text" class="form-control" name="s1_f4" id="s1f4" placeholder="<%= obj.s1.f4.caption %>"
                    value="<%= obj.s1.f4.value %>">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s1_f5" id="s1f5" placeholder="<%= obj.s1.f5.caption %>"
                    value="<%= obj.s1.f5.value %>">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s1_f6" id="s1f6" placeholder="<%= obj.s1.f6.caption %>"
                    value="<%= obj.s1.f6.value %>">
            </div>
        </div>
    </div>
    <% for (var i = 0; i < obj.s2.fams.length; i++) { %>
    <hr class="sidebar-divider">
    <div class="form-group">
        <div class="form-row">
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_nsname_<%= i %>" id="s2famsnsname<%= i %>"
                    placeholder="Name and Surname">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_faname_<%= i %>" id="s2famsfaname<%= i %>"
                    placeholder="Father Name">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_moname_<%= i %>" id="s2famsmoname<%= i %>"
                    placeholder="Mother Name">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_pdbirth_<%= i %>" id="s2famspdbirth<%= i %>"
                    placeholder="Place and Birth Date">
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="form-row">
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_sect_<%= i %>" id="s2famssect<%= i %>"
                    placeholder="sect">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_stat_<%= i %>" id="s2famsstat<%= i %>"
                    placeholder="famsily Status">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_s_<%= i %>" id="s2famss<%= i %>"
                    placeholder="gender">
            </div>
            <div class="form-group col">
                <input type="text" class="form-control" name="s2_fams_remark_<%= i %>" id="s2famsremark<%= i %>"
                    placeholder="remark">
            </div>
        </div>
    </div>
    <% } %>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>