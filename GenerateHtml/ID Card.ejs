                <input class="form-control awesomplete" list="sex" name="s1_f7" id="s1f7"
                    placeholder="<%= obj.s1.f7.caption %>" value="<%= obj.s1.f7.value %>">
                <%if (obj.type == "English") { %>
                <datalist id="sex">
                    <option>male</option>
                    <option>female</option>
                </datalist>
                <% } else if (obj.type == "Français") {%>
                <datalist id="sex">
                    <option>mâle</option>
                    <option>femelle</option>
                </datalist>
                <% } else if (obj.type == "Español") {%>
                <datalist id="sex">
                    <option>masculino</option>
                    <option>femenino</option>
                </datalist>
                <% } else {%>
                <% }%>

            </div>
        </div>
    </div>
